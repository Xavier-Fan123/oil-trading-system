    "@tanstack/react-query": "^5.17.15",
    "@types/ws": "^8.5.10",
    "axios": "^1.6.7",
    "date-fns": "^2.30.0",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.14.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
